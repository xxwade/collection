<html>
<head>
    <title>首页</title>
    <script>
        (function () {
            function o() {
                document.documentElement.style.fontSize = document.documentElement.clientWidth/750 * 100 + 'px';
            }

            var e = null;
            window.addEventListener("resize", function () {
                clearTimeout(e), e = setTimeout(o, 300)
            }, !1), o()
        })(window);
    </script>
    <script src="/js/lib/lsloader.js"></script>
</head>

<body>
<span class="title">首页列表</span>
<div id="root">
    <list :list="listData"></list>
</div>
<a class="title" href="list">二级页面</a>

<script>lsloader.load("<%= common.name %>","<%= common.path %>")</script>
<% if (relyModules.length > 0) { %>
<script>
    (function(){
        var relyModules = [];
        <% relyModules.forEach(function(relyModule){%>
            relyModules.push({
            name : '<%=relyModule.name%>',
            path : '<%=relyModule.path%>'
        });
            <% }) %>
        lsloader.loadCombo(relyModules);
        })()
</script>
<% } %>
<script>lsloader.load("<%= entrance.name %>","<%= entrance.path %>")</script>
</body>
</html>